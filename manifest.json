{
  "manifest_version": 3,
  "name": "XML File Viewer - All Tables",
  "version": "1.1",
  "description": "A Chrome extension to view all XML files at once in table format.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "permissions": [],
  "background": {
    "service_worker": "background.js"
  }
}